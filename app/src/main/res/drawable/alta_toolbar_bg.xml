<?xml version="1.0" encoding="utf-8"?>
<shape xmlns:android="http://schemas.android.com/apk/res/android"
    android:shape="rectangle">
    <corners android:radius="2dp" />
    <gradient
        android:angle="45"
        android:centerColor="#4B6D97"
        android:centerX="35%"
        android:endColor="#4B6D97"
        android:startColor="#4B6D97"
        android:type="linear" />
    <padding
        android:bottom="0dp"
        android:left="0dp"
        android:right="0dp"
        android:top="0dp" />
    <size
        android:width="200dp"
        android:height="50dp" />
    <stroke
        android:width="1dp"
        android:color="#4B6D97" />
</shape>
